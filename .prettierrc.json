{
  "arrayBracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "htmlVoidTags": true,
  "htmlWhitespaceSensitivity": "ignore",
  "offsetTernaryExpressions": true,
  "printWidth": 140,
  "quoteProps": "as-needed",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 4,
  "templateCurlySpacing": true,
  "trailingComma": "all",
  "useTabs": false
}
